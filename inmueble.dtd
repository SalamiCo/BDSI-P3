<!DOCTYPE inmueble [
	<!ELEMENT inmueble (tipo?, operacion?, estado+, info?, opcional?, fotos*)>
	
	<!ELEMENT info (m2?, precio?, provincia?, localidad?)>
	<!ELEMENT opcional (dormitorios?, banos?, piscinaGrande?, piscinaSal?, soleado?)>
	<!ELEMENT fotos >

	<!ELEMENT m2 (#PCDATA)>
	<!ELEMENT precio (#PCDATA)>
	<!ELEMENT provincia (#PCDATA)>
	<!ELEMENT localidad (#PCDATA)>

	<!ELEMENT dormitorios (#PCDATA)>
	<!ELEMENT banos (#PCDATA)>
	<!ELEMENT piscinaGrande (#PCDATA)>
	<!ELEMENT piscinaSal (#PCDATA)>
	<!ELEMENT soleado (#PCDATA)>

	<!ATTLIST anuncio id ID #REQUIRED>
	<!ATTLIST anuncio idagencia IDREFS #REQUIRED>
	<!ATTLIST anuncio tipo (pisos | casas y chalets | solares, terrenos y fincas, locales, oficinas, naves y almacenes | parkings o trasteros) "pisos">
	<!ATTLIST anuncio operacion (venta | alquiler) "venta">
	<!ATTLIST anuncio estado (nuevo | segunda mano buen estado | segunda mano reformado | segunda mano necesita reformar) "nuevo">
]>

